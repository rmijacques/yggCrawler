{

    "manifest_version": 2,
    "name": "yggFollower",
    "version": "1.0",
  
    "description": "search a torrent on ygg from the extension bar",
    "homepage_url": "https://developer.mozilla.org/fr/docs/Mozilla/Add-ons/WebExtensions/Your_second_WebExtension",
    "icons": {
       "48": "icons/main-icon-48.png"
    },
  
    "permissions": [
      "activeTab",
      "storage"
    ],
  
    "browser_action": {
      "default_icon": "icons/main-icon-48.png",
      "default_title": "yggFollower",
      "default_popup": "popup/dashboard/dashboard.html"
    },
  
    "web_accessible_resources": [

    ]
  
  }